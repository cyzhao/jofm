package net.jofm.format;

/**
 * The default <code>Format</code> implementation this is used to distinguish the
 * actually formatter specified with @Field annotation. We don'w want to make formatter
 * attribute of @Field required, and we can not default it to null. 
 * 
 * @author Chunyun Zhao
 * @since 1.0
 */
public class DefaultFormat extends StringFormat {
	public DefaultFormat(Pad pad, char padWith, int length, String format) {
		super(pad, padWith, length, format);
	}
}
